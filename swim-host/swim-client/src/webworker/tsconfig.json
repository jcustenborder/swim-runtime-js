{
  "include": ["**/*"],
  "compilerOptions": {
    "allowUmdGlobalAccess": true,
    "alwaysStrict": true,
    "composite": true,
    "declaration": true,
    "declarationMap": false,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "incremental": true,
    "module": "es2020",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "outDir": "../../lib/webworker",
    "paths": {
      "@swim/client/worker": ["../worker"],
    },
    "preserveConstEnums": true,
    "pretty": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "target": "es2015",
    "tsBuildInfoFile": "../../lib/webworker/.tsbuildinfo",
    "verbatimModuleSyntax": true
  },
  "references": [
    {"path": "../worker"}
  ]
}
